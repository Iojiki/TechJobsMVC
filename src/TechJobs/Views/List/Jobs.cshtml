<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@foreach (Dictionary<string, string> jobs in ViewBag.jobs)
{
    <table class="job-listing">
        @foreach (KeyValuePair<string, string> jobDisplay in jobs)
        {
            <tr>
                <td>@jobDisplay.Key</td>
                <td>@jobDisplay.Value</td>
            </tr>

        }
    </table>

}
